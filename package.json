    "@types/react-dom": "^16.9.6",
    "@types/react-native": "0.61.7",
    "@types/react-native-vector-icons": "^6.4.4",
    "@types/react-test-renderer": "16.9.0",
    "@types/shortid": "0.0.29",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.1.0",
    "babel-preset-expo": "^8.1.0",
    "eslint": "^6.8.0",
    "expo": "^37.0.7",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-dom": "16.9.0",
